package repeticao;

public class ExemploFor {
	public static void main(String[] args) {
		
		/*
		 * Sintaxe For:
		 * - contador e o valor inicial
		 * - condição
		 * - como vai ocorrer o incremento (contagem)
		 */
		
		for (int contador=0;contador<50;contador+=1) {
			System.out.println("Contador: " + contador);
		}























	}
}
